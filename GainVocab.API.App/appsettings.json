{
  "ConnectionStrings": {
    "DefaultDatabase": "(secret.json)"
  },
  "AES": {
    "Key": "(secret.json)"
  },
  "JwtSettings": {
    "Key": "(secret.json)",
    "Issuer": "(secret.json)",
    "Audience": "(secret.json)",
    "DurationInMinutes": "(secret.json)"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./.logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}